		{"id":"87dc4cc8402380b3","type":"text","text":"JDK = JRE + Development Tools\n(Java Development Kit)","x":-289,"y":500,"width":640,"height":460},
		{"id":"68adca7f61da95bc","type":"text","text":"JRE = JVM + Library class\n(Java runtime environment)","x":-229,"y":600,"width":500,"height":340},
